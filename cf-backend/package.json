{
  "name": "kpi-cf-backend",
  "private": true,
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250812.0",
    "csv-parse": "^6.1.0",
    "typescript": "^5.9.2",
    "wrangler": "^4.29.1"
  },
  "scripts": {
    "dev": "wrangler dev --local --port=8787 --persist-to=.wrangler/state",
    "dev:remote": "wrangler dev",
    "deploy": "wrangler deploy",
    "d1:create": "wrangler d1 create kpi-d1",
    "d1:migrate:local": "wrangler d1 migrations apply kpi-d1 --local",
    "d1:migrate:prod": "wrangler d1 migrations apply kpi-d1",
    "d1:console:local": "wrangler d1 execute kpi-d1 --local --command",
    "build:import": "node scripts/import-from-csv.mjs",
    "typecheck": "tsc --noEmit"
  }
}
